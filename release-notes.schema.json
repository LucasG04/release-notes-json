{
 "$schema": "https://json-schema.org/draft/2020-12/schema",
 "title": "Release Notes",
 "type": "array",
 "items":{
   "type": "object",
   "required": ["version", "date", "title", "changes"],
   "properties": {
     "version": {
       "type": "string",
       "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$"
     },
     "date": {
       "type": "string",
       "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
     },
     "title": {
       "type": "string",
       "minLength": 1
     },
     "changes": {
       "type": "array",
       "items": {
         "type": "object",
         "required": ["type", "audience", "description"],
         "properties": {
           "type": {
             "type": "string",
             "enum": ["feature", "fix", "change"]
           },
           "audience": {
             "type": "array",
             "minItems": 1,
             "uniqueItems": true,
             "items": {
               "type": "string",
               "enum": [
                   "ADMIN", "USER", "DEVOPS"
               ]
             }
           },
           "description": {
             "type": "string",
             "minLength": 1
           },
           "imageUrls": {
             "type": "array",
             "items": {
               "type": "string",
               "pattern": "^https://[^\\s/$.?#].[^\\s]*$"
             }
           }
         },
         "additionalProperties": false
       }
     }
   },
   "additionalProperties": false
 }
}
